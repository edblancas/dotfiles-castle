#!/bin/bash
#Generated by JetBrains Toolbox 2.5.3.37797 at 2025-02-14T11:15:13

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/dan/Applications/PyCharm Professional.app/Contents/MacOS/pycharm" $wait --args "${intellij_args[@]}"